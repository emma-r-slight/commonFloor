{
  "name": "commonfloor-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:web": "next dev",
    "build": "npm run build:api && npm run build:web",
    "build:api": "tsc",
    "build:web": "next build web",
    "start": "concurrently \"npm run start:api\" \"npm run start:web\"",
    "start:api": "node dist/index.js",
    "start:web": "next start web",
    "lint": "eslint . --ext .ts --ext .tsx"
  },
  "dependencies": {
    "@daohaus/keychain-utils": "^0.1.1",
    "@daohaus/sdk": "^0.1.2",
    "@tanstack/react-query": "^5.69.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^6.13.0",
    "express": "^4.18.2",
    "lucide-react": "^0.483.0",
    "next": "15.2.3",
    "node-fetch": "^3.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.4",
    "viem": "^2.23.13",
    "wagmi": "^2.14.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20",
    "@types/react": "19.0.12",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5",
    "prettier": "^3.2.5"
  }
}
